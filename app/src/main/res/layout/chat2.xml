<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/eventob"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Inicio"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="-42dp">

    <ImageView
        android:id="@+id/chat2"
        android:layout_width="409dp"
        android:layout_height="142dp"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chat1"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/chat2"
        tools:ignore="ImageContrastCheck" />

    <ImageButton
        android:id="@+id/chat1"
        android:layout_width="412dp"
        android:layout_height="86dp"
        android:background="#FDFCFC"
        android:importantForAccessibility="no"
        android:onClick="Chat"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.004"
        app:srcCompat="@drawable/chat1"
        tools:ignore="VisualLintBounds" />

    <ImageButton
        android:id="@+id/mas_azul"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#F3EEEE"
        android:importantForAccessibility="no"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.975"
        app:srcCompat="@drawable/mas"
        tools:ignore="ImageContrastCheck" />

    <ImageButton
        android:id="@+id/usuarios"
        android:layout_width="228dp"
        android:layout_height="wrap_content"
        android:background="#FFFCFC"
        android:importantForAccessibility="no"
        android:onClick="UsuariosBloqueados"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.715"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.518"
        app:srcCompat="@drawable/bloquear_usuarios" />

    <ImageButton
        android:id="@+id/buscar_usuarios"
        android:layout_width="228dp"
        android:layout_height="wrap_content"
        android:background="#EFEDED"
        android:importantForAccessibility="no"
        android:onClick="BuscarUsuario"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.715"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.632"
        app:srcCompat="@drawable/buscar_usuarios" />

    <ImageButton
        android:id="@+id/Compradas"
        android:layout_width="228dp"
        android:layout_height="wrap_content"
        android:background="#EFEBEB"
        android:importantForAccessibility="no"
        android:onClick="EntradasCompradas"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.715"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.725"
        app:srcCompat="@drawable/entradas_compradas" />

    <ImageButton
        android:id="@+id/Vendidas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#EDECEC"
        android:importantForAccessibility="no"
        android:onClick="EntradasVendidas"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.711"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.84"
        app:srcCompat="@drawable/entradas_vendidas" />

</androidx.constraintlayout.widget.ConstraintLayout>